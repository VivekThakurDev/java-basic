import java.util.*;

public class Main
{
    public static int printMyname(int num1, int num2){
        int sum=num1+num2;
        return sum;
    }
	public static void main(String[] args) {
		Scanner  sc = new Scanner(System.in);
		int num1 = sc.nextInt();
		int num2 = sc.nextInt();
		
		
	 int sum =printMyname(num1,num2);
	 System.out.print("Sum of the two numbers is  "+sum);
	}
}
