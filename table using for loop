// sum of n Number
// BY TAKING INPUT FROM USER
import java.util.*;
  public class Main {
      public static void main( String args[]){
          Scanner sc= new Scanner(System.in);
          int tno =sc.nextInt();
       
          for (int i=1; i<=10 ;i++){
               int number =tno *i;
               System.out.println("2 * "+ i + "= "+number );
          
          }
      }
  }
  // it  give the pass then it check the condition
