import java.util.*;

 public class Main{
  public static void main (String[] args) {
        
        Scanner ag = new Scanner(System.in);
        int age = ag.nextInt();
     if(age >18){
         System.out.println("you are adult");
     }
     else{
         System.out.println("Bacha hai be tum");
     }
    }
}

